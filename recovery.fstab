# mount point	fstype		device			[device2]

/boot		   emmc		/dev/block/mmcblk0p33 flags=display="Boot";backup=1;
/cache		   ext4		/dev/block/mmcblk0p36 flags=display="Cache";backup=1;
/data		   ext4		/dev/block/mmcblk0p37 flags=display="Data";backup=1;
/recovery	   emmc		/dev/block/mmcblk0p34 flags=display="Recovery";backup=1;
/system		   ext4		/dev/block/mmcblk0p35 flags=display="System";backup=1;

# USB OTG DRIVE
/usb-otg  	   vfat		/dev/block/sda1 flags=display="USB-OTG";storage
